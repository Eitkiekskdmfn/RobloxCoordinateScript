-- Scripts:

local function DUHBLVR_fake_script() -- CloseGui.LocalScript 
	local script = Instance.new('LocalScript', CloseGui)

	local Frame = script.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		Frame.Visible = false
	end)
end
coroutine.wrap(DUHBLVR_fake_script)()
local function CEYQRER_fake_script() -- GetCoordinates.LocalScript 
	local script = Instance.new('LocalScript', GetCoordinates)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.CoordText.Text = tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
	end)
end
coroutine.wrap(CEYQRER_fake_script)()
local function PBPGIBG_fake_script() -- Copy.LocalScript 
	local script = Instance.new('LocalScript', Copy)

	script.Parent.MouseButton1Click:Connect(function()
		setclipboard(script.Parent.Parent.CoordText.Text)
	end)
end
coroutine.wrap(PBPGIBG_fake_script)()
local function MUHZINC_fake_script() -- OpenGui.LocalScript 
	local script = Instance.new('LocalScript', OpenGui)

	local Frame = script.Parent.Parent.MainFrame
	
	script.Parent.MouseButton1Click:Connect(function()
		Frame.Visible = true
	end)
end
coroutine.wrap(MUHZINC_fake_script)()
