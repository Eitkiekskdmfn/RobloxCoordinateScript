-- Scripts:

local function VLGXPOU_fake_script() -- CloseScript.LocalScript 
	local script = Instance.new('LocalScript', CloseScript)

	local Frame = script.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		Frame.Visible = false
	end)
end
coroutine.wrap(VLGXPOU_fake_script)()
local function NTBUB_fake_script() -- GetCoordinates.LocalScript 
	local script = Instance.new('LocalScript', GetCoordinates)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.CoordText.Text = tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
	end)
end
coroutine.wrap(NTBUB_fake_script)()
local function KCYFAX_fake_script() -- Copy.LocalScript 
	local script = Instance.new('LocalScript', Copy)

	script.Parent.MouseButton1Click:Connect(function()
		setclipboard(script.Parent.Parent.CoordText.Text)
	end)
end
coroutine.wrap(KCYFAX_fake_script)()
