Frame.Active = true
Frame.Draggable = true

CloseScript.Name = "CloseScript"
CloseScript.Parent = Frame
CloseScript.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
CloseScript.Position = UDim2.new(0.917142868, 0, 0, 0)
CloseScript.Size = UDim2.new(0, 29, 0, 32)
CloseScript.Font = Enum.Font.SourceSans
CloseScript.Text = "X"
CloseScript.TextColor3 = Color3.fromRGB(0, 0, 0)
CloseScript.TextSize = 30.000

CoordText.Name = "CoordText"
CoordText.Parent = Frame
